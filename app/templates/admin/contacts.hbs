<h1>Contacts</h1>


<div class="row">
  {{#each model as |contacts|}}
  <div class="col-md-4">
    <div class="panel panel-default library-item">
      <div class="panel-heading">
        <h3 class="panel-title">{{contacts.email}}</h3>
      </div>
      <div class="panel-body">
        <p>ID: {{contacts.id}}</p>
        <p>Email: {{contacts.email}}</p>
      </div>
      <div class="panel-footer text-right">
        <button class="btn btn-danger btn-xs" {{action 'deleteContact' contacts}}>Delete</button>
      </div>
    </div>
  </div>
  {{/each}}
</div>
